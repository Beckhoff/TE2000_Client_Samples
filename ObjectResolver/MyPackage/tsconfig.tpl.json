{
  "$schema": "http://json.schemastore.org/tsconfig",
  "compileOnSave": true,
  "compilerOptions": {
    "module": "none",
    "target": "ES2022",
    "skipLibCheck": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ES2023"
    ],
    "types": [],
    "declaration": true,
    "sourceMap": true,
    "noEmitOnError": true,
    "noImplicitAny": false,
    "noImplicitThis": true,
    "strictBindCallApply": true,
    "strictNullChecks": true,
    "noImplicitReturns": true,
    "strictFunctionTypes": false,
    "strictPropertyInitialization": false,
    "alwaysStrict": false
  },
  "include": [
    "$(Beckhoff.TwinCAT.HMI.Framework).InstallPath/TcHmi.d.ts",
    "$(Beckhoff.TwinCAT.HMI.Framework).InstallPath/Controls/System/TcHmiControl/TcHmiControl.d.ts",
    "$(Beckhoff.TwinCAT.HMI.Controls).InstallPath/TcHmiNumericInput/TcHmiNumericInput.d.ts",
    "$(Beckhoff.TwinCAT.HMI.Controls).InstallPath/TcHmiSpinboxInput/TcHmiSpinboxInput.d.ts"
  ],
  "files": [
    "MyControl/MyControl.ts"
  ]
}